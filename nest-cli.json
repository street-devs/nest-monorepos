{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "root": "apps/cms",
  "sourceRoot": "apps/cms/src",
  "monorepo": true,
  "defaultLibraryPrefix": "@lib",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/cms/tsconfig.app.json"
  },
  "projects": {
    "cms": {
      "type": "application",
      "root": "apps/cms",
      "entryFile": "main",
      "sourceRoot": "apps/cms/src",
      "compilerOptions": {
        "tsConfigPath": "apps/cms/tsconfig.app.json"
      }
    },
    "nest-app": {
      "type": "library",
      "root": "libs/nest-app",
      "entryFile": "index",
      "sourceRoot": "libs/nest-app/src",
      "compilerOptions": {
        "tsConfigPath": "libs/nest-app/tsconfig.lib.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "monitoring": {
      "type": "library",
      "root": "libs/monitoring",
      "entryFile": "index",
      "sourceRoot": "libs/monitoring/src",
      "compilerOptions": {
        "tsConfigPath": "libs/monitoring/tsconfig.lib.json"
      }
    }
  }
}
